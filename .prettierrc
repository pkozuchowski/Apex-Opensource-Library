{
    "plugins": ["prettier-plugin-apex"],
    "trailingComma": "none",
    "tabWidth": 4,
    "overrides": [
      {
        "files": "**/lwc/**/*.html",
        "options": { "parser": "lwc" }
      },
      {
        "files": "*.{cmp,page,component}",
        "options": { "parser": "html" }
      }
    ]
  }